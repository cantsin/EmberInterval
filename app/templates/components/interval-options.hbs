<div class="row-fluid">
  <div class="col-md-12">
    <strong>
      Time Interval:
    </strong>
    {{elapsed.days}} days {{elapsed.hours}} hours {{elapsed.minutes}} minutes
  </div>
</div>

<div class="row-fluid">
  <div class="col-md-12">
    <strong>
      Options
    </strong>
    <ul>
      {{#each options as |item|}}
      <li>
        Option {{item}}
      </li>
      {{else}}
      <li>
        No options available.
      </li>
      {{/each}}
    </ul>
  </div>
</div>
